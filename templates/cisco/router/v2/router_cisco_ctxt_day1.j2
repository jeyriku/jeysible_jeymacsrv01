hostname

logging buffered 999999
no aaa new-model
clock timezone CET 1 0
clock summer-time CEST recurring last Sun Mar 2:00 last Sun Oct 3:00
!
boot system bootflash:c1100-universalk9.17.08.01a.SPA.bin
!
ip name-server 192.168.0.252 192.168.0.250
ip domain lookup source-interface Loopback0
ip domain name int.jeyriku.net
!
no cdp run
!
lldp run
!
interface Loopback0
 ip address 10.0.1.12 255.255.255.255
 ip ospf 4 area 0
!
interface GigabitEthernet0/0/0
 description Lien vers jey-srx3x-pe-02
 mtu 1600
 ip address 192.168.11.1 255.255.255.252
 ip ospf 4 area 0
 negotiation auto
!
interface GigabitEthernet0/0/1
 description Lien vers jey-srx3x-ce-01
 ip address 192.168.14.1 255.255.255.252
 ip ospf network point-to-point
 ip ospf 4 area 0
 media-type rj45
 negotiation auto
!
interface GigabitEthernet0/1/0
 description Lien vers jey-c2960-sw-05
!
interface GigabitEthernet0/1/1
 shutdown
!
interface GigabitEthernet0/1/2
 shutdown
!
interface GigabitEthernet0/1/3
 shutdown
!
interface GigabitEthernet0/1/4
 shutdown
!
interface GigabitEthernet0/1/5
 shutdown
!
interface GigabitEthernet0/1/6
 shutdown
!
interface GigabitEthernet0/1/7
 shutdown
!
interface Vlan1
 description Lien vers Inet-JeyWanFree-SFR
 ip address 192.168.10.4 255.255.255.248
!

!
no ip access-list standard 18
access-list 18 permit host 192.168.0.248
access-list 18 permit host 192.168.0.249
access-list 18 permit host 192.168.0.239
snmp-server view Jeyriku system included
snmp-server view Jeyriku interfaces included
snmp-server view Jeyriku system.1 included
snmp-server community {{ device_name.msg[item].snmp_com }} RO 18
snmp-server location "{{ device_name.msg[item].snmp_loc }}"
snmp-server enable traps snmp linkdown linkup coldstart
snmp-server host {{ device_name.msg[item].snmp_srv1 }} {{ device_name.msg[item].snmp_com }}
snmp-server host {{ device_name.msg[item].snmp_srv2 }} {{ device_name.msg[item].snmp_com }}
snmp-server host {{ device_name.msg[item].snmp_srv3 }} {{ device_name.msg[item].snmp_com }}

logging host {{ ciscodevices.msg[item].snmp_srv2 }}
logging trap informational
logging history informational
logging origin-id hostname
facility local6
logging host {{ ciscodevices.msg[item].snmp_srv2 }} transport udp port 514
service sequence numbers
service timestamps log
!
banner motd ^CCCCCCCCCCCC


        YOU ARE ABOUT TO CONNECT TO A RESTRICTED AREA
        ***********************************************************************

                  Unauthorized access prohibited


        ***********************************************************************
^C
!
line con 0
 exec-timeout 0 0
 login local
 transport input none
 escape-character 17
 stopbits 1
line vty 0 4
 exec-timeout 0 0
 login local
 transport input telnet ssh
 escape-character 17
line vty 5 15
 exec-timeout 0 0
 login local
 transport input telnet ssh
 escape-character 17
!
ntp server 192.168.0.250
ntp server 192.168.0.252
!
netconf ssh
!
!
!
!
!
netconf-yang
netconf-yang feature candidate-datastore
restconf
